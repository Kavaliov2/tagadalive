include('model/Fight')
include('model/Map')
include('model/Items')
include('model/Entity')
include('model/Item')
include('model/ItemEffect')
include('model/Action')
include('model/Cell')
include('model/MapDanger')
include('model/MapAction')
include('model/Combo')
include('model/Danger')
include('model/TargetType')
include('model/Consequences')

include('API/compatibility')

function init() {
	if(getTurn()==1) {
		Map.init()
		Items.init()
	}
	Fight.refresh()
	Map.refresh()
	MapDanger.refresh()
	MapAction.refresh()
}

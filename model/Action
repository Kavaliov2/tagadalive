/*
 * Action réalisable d'une entité vers une autre
 */
class Action {
	public from
	public to
	public item
	constructor(item, from, to){
		this.item = item
		this.from = from
		this.to = to
	}
	
	/*
	 * Format chaîne de caractères utilisée pour des tests / debugs.
	 */
	string(){
		return "<Action "+this.item+":"+this.from+":"+this.to+">"
	}
}